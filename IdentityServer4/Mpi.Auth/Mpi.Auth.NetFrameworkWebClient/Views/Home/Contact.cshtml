@using System.Security.Claims;
@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<dl>
    @foreach (var claim in (User as ClaimsPrincipal).Claims) {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>

    }
